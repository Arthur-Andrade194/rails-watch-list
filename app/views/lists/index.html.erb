

<div class="container">
  <h1 class="mb-4">Lists</h1>
  <div class="list-group">
    <% @list.each do |list| %>
      <%= link_to list.name, list_path(list), class: 'list-group-item list-group-item-action' %>
    <% end %>
  </div>
  <%= link_to "Create a movie list", new_list_path, class: "btn btn-flat" %>
</div>
